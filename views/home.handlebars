  <div class="container">
       <h1>Lista de Contatos</h1>
       <table class="table">
           <thead>
               <tr>
                   <th>Nome</th>
                   <th>Email</th>
                   <th>Telefone</th>
               </tr>
           </thead>
           <tbody>
            {{!-- LÃª o array de contatos --}}
               {{#each contatos}}
                   <tr>
                       <td>{{this.nome}}</td>
                       <td>{{this.email}}</td>
                       <td>{{this.telefone}}</td>
                   </tr>
               {{/each}}
           </tbody>
       </table>
   </div>